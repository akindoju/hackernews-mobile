{
  "expo": {
    "name": "Hacker Gist",
    "slug": "hacker-gist",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/hackerNewsLogo.png",
    "splash": {
      "image": "./assets/hackerNewsSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ff6600"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.akindoju.hacker_gist",
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/hackerNewsLogo.png",
        "backgroundColor": "#ff6600"
      },
      "permissions": ["WRITE_EXTERNAL_STORAGE", "READ_EXTERNAL_STORAGE"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
